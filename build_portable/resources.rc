        BLOCK "040904B0"
        BEGIN
            VALUE "CompanyName",        "HEIMLICH"
            VALUE "FileDescription",   "Player by HEIMLICH®"
            VALUE "FileVersion",       "0.0.2"
            VALUE "InternalName",      "Player by HEIMLICH"
            VALUE "LegalCopyright",    "Copyright (C) 2025 HEIMLICH®"
            VALUE "OriginalFilename",  "Player-by-HEIMLICH.exe"
            VALUE "ProductName",       "Player by HEIMLICH®"
            VALUE "ProductVersion",    "0.0.2"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END 
